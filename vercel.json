{
      "version": 2,
      "builds": [
      {
      "src": "poll/wsgi.py", 
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
      "src": "build_files.sh",
      "use": "@vercel/static-build", "config": {
      "distDir": "staticfiles_build"
      }
     }
    ],

    "routes": [
     {
      "src": "/(.vercel.app)",
      "dest": "/static/"
     },
     {
      "src":"/(.vercel.app)",
      "dest": "poll/wsgi.py"
      }
     ]
    }
